<?xml version="1.0" encoding="utf-8"?>
<com.github.ksoichiro.android.observablescrollview.ObservableScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/payThePiperScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/material_view_pager_placeholder"/>

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="12dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/back"
                android:layout_centerHorizontal="true"
                android:id="@+id/relativeLayout">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="#0d47a1"
                    android:id="@+id/relativeLayout2"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Description "
                        android:layout_marginTop="6dp"
                        android:textColor="#ffffff"
                        android:id="@+id/textView2"
                        android:layout_centerInParent="true" />



                </RelativeLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="120sp"
                    android:textSize="16sp"
                    android:textColor="#000000"
                    android:padding="5sp"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="A business face off between competing teams . The challenge is to technically convince the judges to buy the car using imaginative build up with technical support and at a higher price compared to other teams ."
                    android:id="@+id/textView3"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_below="@+id/relativeLayout2" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:background="@drawable/back"
                android:layout_marginBottom="18dp"
                android:layout_alignLeft="@+id/relativeLayout"
                android:layout_alignStart="@+id/relativeLayout"
                android:layout_below="@+id/relativeLayout"
                android:layout_alignRight="@+id/relativeLayout"
                android:layout_alignEnd="@+id/relativeLayout"
                android:id="@+id/relativeLayout3">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="38dp"
                    android:id="@+id/imageView"
                    android:src="@drawable/regis"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_above="@+id/imageView2" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Registration fee  :"
                    android:textStyle="normal"
                    android:layout_marginTop="6dp"
                    android:textColor="#000000"
                    android:id="@+id/textView4"
                    android:layout_alignParentTop="true"
                    android:layout_toEndOf="@+id/imageView"
                    android:layout_toRightOf="@+id/imageView" />

                <TextView
                    android:layout_width="wrap_content"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rs.100"
                    android:id="@+id/textView5"
                    android:layout_alignTop="@+id/textView4"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="45dp"
                    android:layout_marginEnd="45dp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="38dp"
                    android:id="@+id/imageView2"
                    android:src="@drawable/second"
                    android:layout_above="@+id/imageView3"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="38dp"
                    android:id="@+id/imageView3"
                    android:src="@drawable/second"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="First Prize  :"
                    android:textStyle="normal"
                    android:id="@+id/textView6"
                    android:textColor="#000000"
                    android:layout_below="@+id/imageView"
                    android:layout_alignLeft="@+id/textView4"
                    android:layout_alignStart="@+id/textView4" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Second Prize  :"
                    android:textStyle="normal"
                    android:id="@+id/textView7"
                    android:textColor="#000000"
                    android:layout_below="@+id/imageView2"
                    android:layout_alignLeft="@+id/textView6"
                    android:layout_alignStart="@+id/textView6" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rs.2500"
                    android:id="@+id/textView8"
                    android:layout_alignTop="@+id/textView6"
                    android:layout_alignLeft="@+id/textView5"
                    android:layout_alignStart="@+id/textView5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rs.1500"
                    android:textColor="#000000"
                    android:id="@+id/textView9"
                    android:layout_alignTop="@+id/textView7"
                    android:layout_alignLeft="@+id/textView8"
                    android:layout_alignStart="@+id/textView8" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="250dp"
                android:background="@drawable/back"
                android:layout_below="@+id/relativeLayout3"
                android:layout_alignRight="@+id/relativeLayout3"
                android:layout_alignEnd="@+id/relativeLayout3"
                android:layout_alignLeft="@+id/relativeLayout3"
                android:layout_alignStart="@+id/relativeLayout3"
                android:id="@+id/relativeLayout6">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="#0d47a1"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:id="@+id/relativeLayout4">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Coordinators "
                        android:textColor="#ffffff"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/textView16" />

                </RelativeLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView4"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/contact"
                    android:layout_marginTop="20dp"
                    android:layout_below="@+id/relativeLayout4"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Keerthi Prakash "
                    android:textColor="#000000"
                    android:id="@+id/textView10"
                    android:layout_alignTop="@+id/imageView4"
                    android:layout_centerHorizontal="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#008000"
                    android:padding="4dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="9482708205 "
                    android:id="@+id/textView11"
                    android:layout_alignBottom="@+id/imageView4"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView5"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/contact"
                    android:layout_alignTop="@+id/textView13"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="#000000"

                    android:id="@+id/relativeLayout5"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/imageView4"></RelativeLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Kalyan C "
                    android:id="@+id/textView13"
                    android:layout_marginTop="36dp"
                    android:layout_below="@+id/relativeLayout5"
                    android:layout_alignLeft="@+id/textView11"
                    android:layout_alignStart="@+id/textView11" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#008000"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="8050792685 "
                    android:id="@+id/textView14"
                    android:layout_alignBottom="@+id/imageView5"
                    android:layout_alignLeft="@+id/textView10"
                    android:layout_alignStart="@+id/textView10" />

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/relativeLayout6"
                android:background="@drawable/back"
                android:layout_marginTop="18dp"
                android:layout_alignLeft="@+id/relativeLayout6"
                android:layout_alignStart="@+id/relativeLayout6"
                android:layout_alignRight="@+id/relativeLayout6"
                android:layout_alignEnd="@+id/relativeLayout6">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="#0d47a1"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:id="@+id/relativeLayout7">
                    <TextView
                        android:layout_width="wrap_content"
                        android:textStyle="bold"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Rules "
                        android:textColor="#ffffff"
                        android:layout_centerInParent="true"
                        />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:layout_below="@+id/relativeLayout7"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/rules_pay"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:layout_margin="2dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        />

                </RelativeLayout>

            </RelativeLayout>


        </RelativeLayout>

    </LinearLayout>

</com.github.ksoichiro.android.observablescrollview.ObservableScrollView>